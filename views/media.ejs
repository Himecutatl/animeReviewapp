<table id="list">
    <thead>
      <tr>
        <th>Title</th>
        <th>Release<br>Year</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% media.forEach(function(m) { %>
        <tr>
          <td><%= m.title %></td>
          <td><%= m.releaseYear %></td>
          <td><a href="/media/<%= m._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>